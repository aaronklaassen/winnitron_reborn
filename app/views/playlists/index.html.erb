<p id="notice"><%= notice %></p>

<h1>Playlists</h1>

<p>
  <%= link_to "New Playlist", new_playlist_path, class: "btn btn-success" %>
</p>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @playlists.each do |playlist| %>
      <tr>
        <td><%= link_to playlist.title, playlist %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


