<div class="dash">
  <h1>Whatcha up to?</h1>

  <div class="dash-item panel panel-default">
    <div class="panel-body">
      <a href="<%= new_game_path %>">
        <%= image_tag "rocket-ship", class: "icon" %>
        <span class="text">Submit a game</span>
      </a>
    </div>
  </div>

  <div class="dash-item panel panel-default">
    <div class="panel-body">
      <a href="<%= new_arcade_machine_path %>">
        <%= image_tag "game-console", class: "icon" %>
        <span class="text">Register a Winnitron</span>
      </a>
    </div>
  </div>

  <div class="dash-item panel panel-default">
    <div class="panel-body">
      <a href="<%= new_playlist_path %>">
        <%= image_tag "add-list", class: "icon" %>
        <span class="text">Make a playlist</span>
      </a>
    </div>
  </div>

  <div class="dash-item panel panel-default">
    <div class="panel-body">
      <a href="<%= "/" %>">
        <%= image_tag "heart-list", class: "icon" %>
        <span class="text">Manage playlist subscriptions</span>
      </a>
    </div>
  </div>

</div>
