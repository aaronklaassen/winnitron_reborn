<p id="notice"><%= notice %></p>

<h1>Games</h1>

<p>
  <%= link_to "Add your Game!", new_game_path, class: "btn btn-success" %>
</p>


<table class="table table-condensed">

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><strong><%= link_to game.title, game_path(game) %></strong></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


